    "version": "0.2.0",
    "configurations": [
    
            {
      "type": "chrome", // or "edge", depending on your browser
      "request": "launch",
      "name": "Launch Vite Frontend",
      "url": "http://localhost:5173", // Replace with your Vite dev server URL
